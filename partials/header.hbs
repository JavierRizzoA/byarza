<header>
  <nav>
    <div class="nav-left">
      {{#if @site.logo}}
        <a href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" class="nav-logo" /></a>
      {{else}}
        <a href="{{@site.url}}">{{@site.title}}</a>
      {{/if}}
    </div>
    <div class="nav-right">
      {{#if @site.navigation}}
        {{navigation}}
      {{/if}}
    </div>
  </nav>
</header>
